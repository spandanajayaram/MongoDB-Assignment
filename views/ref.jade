extends layout

var ref

block list
  br
  br
  table
    tr
      td 
        a(href='items') TV 
      td
        a(href='book') Books 
      td 
        a(href='cosmetic') Cosmetic
      td 
        a(href='ref') Refrigerator
 

block content
